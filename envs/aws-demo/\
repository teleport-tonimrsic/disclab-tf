terraform {
  required_version = ">= 1.14.0"

  required_providers {
    aws = {
      source = "hashicorp/aws"
      version = "~> 5.0"
    }
  }
}

# Variables

variable "region" {
  type = string
  description = "AWS region to deploy into."
  default = "eu-west-1"
}

variable "creator_email" {
  type = string
  description = "Value for teleport.dev/creator tag"
  default = "toni.mrsic@goteleport.com"
}


